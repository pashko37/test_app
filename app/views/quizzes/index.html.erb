<p id="notice"><%= notice %></p>

<h1>Quizzes</h1>

<table>
  <thead>
    <tr>
      <th>Matter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quizzes.each do |quiz| %>
      <tr>
        <td><%= quiz.title %></td>
        <td><%= quiz.desc %></td>
        <td><%= link_to 'Go to Quiz', quiz %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
