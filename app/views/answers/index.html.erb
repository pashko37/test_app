<p id="notice"><%= notice %></p>

<h1>Answers</h1>

<table>
  <thead>
    <tr>
      <th>Matter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.matter %></td>
        <td><%= link_to 'Go to Quiz', answer %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
